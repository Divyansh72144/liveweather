ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# API Configuration
OPENMETEO_API=https://api.open-meteo.com/v1/forecast

# Rate Limiting
# Maximum requests per windowMs
RATE_LIMIT_MAX=100
# Time window in milliseconds (15 minutes)
RATE_LIMIT_WINDOW=900000
